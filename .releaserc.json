{
  "semantic_release": {
    "assets": [],
    "branches": {
      "main": {
        "match": "(main|master)"
      }
    },
    "commit_message": "{version}\n\nAutomatically generated by python-semantic-release",
    "commit_parser": "angular",
    "commit_parser_options": {
      "allowed_tags": [
        "build",
        "chore",
        "ci",
        "docs",
        "feat",
        "fix",
        "perf",
        "style",
        "refactor",
        "test"
      ],
      "minor_tags": ["feat"],
      "patch_tags": ["fix", "perf"],
      "default_bump_level": 0
    },
    "tag_format": "v{version}",
    "publish": {
      "upload_to_vcs_release": true
    }
  }
}
